{
  "name": "worms",
  "version": "1.0.0",
  "description": "WormsClone by Inikon",
  "main": "index.js",
  "scripts": {
    "test": "npx jest ./test/test.js",
    "lint": "npx eslint ./src",
    "fix": "npx eslint ./src --fix",
    "prod": "npx sass ./src/scss/main.scss style-build/main.css && npx webpack --mode production",
    "dev": " npx sass ./src/scss/main.scss style-build/main.css && npx webpack --mode development",
    "start-js": "npx webpack-dev-server",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar ./src/scss/main.scss -o style-build/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/scss/main.scss -o style-build/main.css --watch --recursive"
  },
  "author": "Inikon",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
